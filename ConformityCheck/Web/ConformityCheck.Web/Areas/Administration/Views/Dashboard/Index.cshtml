@model ConformityCheck.Web.ViewModels.Administration.Dashboard.IndexViewModel
@{
    this.ViewData["Title"] = "Admin dashboard";
}

<section class="display-data-wrapper">
    <section class="display-data-container">
        <section class="display-data-header">
            <article class="title-container">
                <h4 class="title">
                    @this.ViewData["Title"]
                </h4>
            </article>
        </section>

        <section class="display-data-body">
            <article class="display-data-body data-container">
                <p class="center">
                    Settings: @this.Model.SettingsCount
                </p>
            </article>
        </section>

        <section class="display-data-footer">
        </section>
    </section>
</section>
