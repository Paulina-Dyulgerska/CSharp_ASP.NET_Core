@{
    this.ViewData["Title"] = "Admin";
}

<section class="display-data-wrapper">
    <section class="display-data-container">
        <section class="display-data-header">
            <article class="title-container">
                <h4 class="title">
                    @this.ViewData["Title"]
                </h4>
            </article>
        </section>
        <section class="large-margin-vertical">
            <nav>
                <ul class="btns-container nav-list">
                    <li>
                        <a asp-area="Administration" asp-controller="Dashboard" asp-action="Index" class="btn btn-cta">
                            Dashboard
                        </a>
                    </li>
                    <li>
                        <a asp-area="Administration" asp-controller="Users" asp-action="Index" class="btn btn-cta">
                            Users
                        </a>
                    </li>
                </ul>
            </nav>
        </section>
        <section class="display-data-body">
        </section>
        <section class="display-data-footer">
        </section>
    </section>
</section>