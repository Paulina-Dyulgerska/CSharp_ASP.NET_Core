@using ConformityCheck.Common
@model ConformityCheck.Web.ViewModels.Home.IndexViewModel

@{
    this.ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to @GlobalConstants.SystemName!</h1>
    <div class="col-md-4 offset-md-4">
        <p>We have:</p>
        <ul class="list-group">
            <li class="list-group-item bg-info">Articles: @Model.Articles</li>
            <li class="list-group-item bg-light">Products: @Model.Products</li>
            <li class="list-group-item bg-info">Suppliers: @Model.Suppliers</li>
            <li class="list-group-item bg-light">ConformityTypes: @Model.ConformityTypes</li>
            <li class="list-group-item bg-info">Conformities: @Model.Conformities</li>
            <li class="list-group-item bg-light">Substances: @Model.Substances</li>
            <li class="list-group-item bg-info">RegulationLists: @Model.RegulationLists</li>
        </ul>
    </div>
</div>

<div class="btn-toolbar mb-3" role="toolbar" aria-label="Toolbar with button groups">
    <h4 class="m-0 font-weight-bold text-primary ">
        @this.ViewData["Title"]
    </h4>
    <div class="input-group">
        <div class="input-group-prepend">
            <div class="input-group-text" id="btnGroupAddon">
                <i class="fas fa-search"></i>
            </div>
        </div>
        <input size="36" type="text" id="searchInput" class="form-control" placeholder="Search for article number or name..." aria-label="Input group example" aria-describedby="btnGroupAddon">
    </div>
</div>

<div class="d-flex justify-content-between mb-3" role="toolbar">
    <h4 class="m-0 font-weight-bold text-primary ">
        @this.ViewData["Title"]
    </h4>

    <div class="btn-toolbar mb-3" role="toolbar" aria-label="Toolbar with button groups">
        <div class="input-group">
            <div class="input-group-prepend">
                <div class="input-group-text" id="btnGroupAddon">
                    <i class="fas fa-search"></i>
                </div>
            </div>
            <input size="36" type="text" id="searchInput" class="form-control" placeholder="Search for article number or name..." aria-label="Input group example" aria-describedby="btnGroupAddon">
        </div>
    </div>

    <a asp-action="Create" class="btn btn-primary justify-content-end">Create article</a>
</div>
